<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
  <script src="https://cdn.tailwindcss.com"></script>

</head>
<body> 

  <% if(nav === true){ %>
    <div class="w-full px-10 py-3 flex justify-between bg-zinc-800 text-white">
        <h3 class="text-xl"><i>Tech Titan</i></h3>
        <div class="flex gap-10 items-center">
            <a href="/profile">Profile</a>
            <a href="/feed">Feed</a>
            <a href="/logout" class="px-3 py-2 bg-red-700 etxt-white rounded-lg">Logout</a>
        </div>
    </div>
    <% } %>
    <div class="w-full min-h-screen bg-zinc-800 px-10 py-10 text-white">
       <div class="cards flex flex-wrap gap-10">
        <% user.posts.forEach(function(elem){ %>
            <div class="card w-32">
               <div class="card w-32 h-40 bg-sky-200 rounded-lg overflow-hidden">
                   <img class="w-full" src="/images/uploads/<%= elem.image %>">
               </div>
               <h5 class="mt-2 text-sm"><%= elem.title %> </h5>
            </div>
         <%  }) %>
       </div>
    </div>
    
</body>
</html>










